    "node": "6.11.2"
  },
  "scripts": {
    "start": "node server",
    "start1": "concurrently --kill-others \"npm run _server:run\" \"ng serve --delete-output-path=false --proxy-config proxy.conf.json\"",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "test:client": "ng test",
    "e2e:client": "ng e2e",
    "build": "",
    "deploy": "npm run obuild && cd azure && git commit -am \"auto commit\" && git push azure master",
    "obuild": "DEL azure\\* /Q && ng build --output-path=dist/ && npm run _server:build && xcopy dist azure /E /Y && copy package.json azure\\ && copy web.config azure\\",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/server.js\" ",
    "_server:build": "tsc -p ./server",
    "postinstall": "npm run build",
    "ng": "ng",
    "ng-start": "ng serve",
    "ng-build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
